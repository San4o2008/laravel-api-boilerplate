{
    "name": "nixsolutions/laravel-api-boilerplate",
    "description": "Laravel api skeleton",
    "keywords": ["json-api"],
    "license": "",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.4.*",
        "darkaonline/l5-swagger": "4.0.1",
        "cloudcreativity/laravel-json-api": "^0.8",
        "tymon/jwt-auth": "1.0.0-beta.3",
        "barryvdh/laravel-ide-helper": "^2.2",
        "barryvdh/laravel-debugbar": "^2.3",
        "doctrine/dbal": "^2.5",
        "laravel/homestead": "^5.0",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.3.*",
        "symfony/dom-crawler": "3.3.*",
        "mikey179/vfsStream": "1.6.4",
        "xethron/migrations-generator": "^2.0",
        "satooshi/php-coveralls": "dev-master",
        "laracasts/generators": "dev-master as 1.1.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "repo-name": {
            "type": "vcs",
            "url": "https://github.com/jamisonvalenta/Laravel-4-Generators.git"
        }
    }
}
